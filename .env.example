# Deep Research Telegram Bot Environment Variables
# Copy this file to .env and fill in your API keys

# LLM API (overrides config.yaml llm.api_key)
ANTHROPIC_API_KEY=sk-...

# Tavily Search API (overrides config.yaml search.api_key)
TAVILY_API_KEY=tvly-...

# Telegram Bot Token (overrides config.yaml telegram.bot_token)
TELEGRAM_BOT_TOKEN=123456789:ABC...

# Langfuse Observability (optional)
# Get keys at https://cloud.langfuse.com
LANGFUSE_ENABLED=false
LANGFUSE_PUBLIC_KEY=pk-lf-...
LANGFUSE_SECRET_KEY=sk-lf-...
LANGFUSE_BASE_URL=https://cloud.langfuse.com
